@model IEnumerable<Category>

<div class="list-group">
    @foreach (var item in Model)
    {
        <a 
            asp-controller="Posts"
            asp-action="Index"
            asp-route-id="@item.Id"
            class="list-group-item list-group-item-action " aria-current="true">
            @item.Name
        </a>

    }

</div>